{
    "name":"Youtube Video Downloader Latest 2020",
    "description":"Now Download videos in simply 2 clicks!!",
    "version":"0.1",
    "author":"Praful Prasad",
    "manifest_version":2,
    "web_accessible_resources":["src/youtubedl.js"],

    "content_scripts":[
        {
            "matches":["https://www.youtube.com/watch*"],
            "js":["src/loader.js"],
            "css":["src/swaggify.css"]
        }
    ],
    "permissions":["downloads"],
    "background":{
        "scripts":["src/background.js"]
    }
}



